

<DrawerClickableItem@MDNavigationDrawerItem>
    focus_color: "#e7e4c0"
    text_color: "#4a4939"
    icon_color: "#4a4939"
    ripple_color: "#c5bdd2"
    selected_color: "#0c6c4d"


<DrawerLabelItem@MDNavigationDrawerItem>
    text_color: "#4a4939"
    icon_color: "#4a4939"
    focus_behavior: False
    selected_color: "#4a4939"
    _no_ripple_effect: True




<ContentNavigationDrawer>:
    orientation: "vertical"
    padding: "8dp"
    spacing: "8dp"


    MDNavigationDrawerMenu:

        MDNavigationDrawerHeader:

            title: "Мой профиль"
            title_color: "#4a4939"
            spacing: "4dp"
            padding: "8dp", 0, 0, "80dp"

        DrawerClickableItem:
            icon: "gmail"
            text_right_color: "#4a4939"
            text: "оформить заказ"
            on_release: app.order1()

        DrawerClickableItem:
            icon: "send"
            text: "предыдущие заказы"
            on_release: app.order2()

        DrawerClickableItem:
            icon: "logout"
            text_right_color: "#4a4939"
            text: "выйти из аккаунта"
            on_release: app.back('screen1')




